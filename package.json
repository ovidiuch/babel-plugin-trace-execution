{
  "name": "babel-plugin-trace-execution",
  "version": "0.1.1",
  "description": "Trace execution context of JS functions",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/skidding/babel-plugin-trace-execution.git"
  },
  "main": "lib/index.js",
  "scripts": {
    "lint": "xo",
    "test": "npm run lint",
    "build": "babel src -d lib",
    "prepublish": "npm run test && npm run build"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-preset-es2015": "^6.18.0",
    "xo": "^0.17.1"
  },
  "xo": {
    "space": true,
    "esnext": true,
    "rules": {
      "comma-dangle": 0,
      "object-curly-spacing": 0
    }
  },
  "dependencies": {
    "babel-template": "^6.16.0",
    "lodash.clonedeep": "^4.5.0"
  }
}
